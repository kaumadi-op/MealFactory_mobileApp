<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Order">

    <LinearLayout
        android:layout_width="509dp"
        android:layout_height="72dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="146dp"
            android:layout_height="match_parent"
            android:background="#FAF8F8"
            app:srcCompat="@drawable/whatsapp_image_2020_08_13_at_23_22_18" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="95dp"
            android:layout_height="match_parent"
            android:background="#F8F7F7"
            app:srcCompat="@drawable/whatsapp_image_2020_08_13_at_23_22_17__1_" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="118dp"
            android:layout_height="match_parent"
            android:background="#FBFBFB"
            app:srcCompat="@drawable/whatsapp_image_2020_08_13_at_23_22_17" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="93dp"
            android:layout_height="match_parent"
            android:background="#FBFBFB"
            app:srcCompat="@drawable/whatsapp_image_2020_08_13_at_23_22_18__1_" />

    </LinearLayout>

    <Button
        android:id="@+id/takeaway"
        android:layout_width="115dp"
        android:layout_height="61dp"
        android:background="#F44336"
        android:text="Takeaway"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.79"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/delivery"
        android:layout_width="115dp"
        android:layout_height="61dp"
        android:background="#F44336"
        android:text="Delivery"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.226"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LET'S START ORDERING"
        android:textColor="#F44336"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.263" />

    <View
        android:id="@+id/divider5"
        android:layout_width="483dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.135" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meal Factory"
        android:textColor="#F44336"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.696"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.053" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="84dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.204"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/whatsapp_image_2020_08_11_at_16_14_41" />

    <Button
        android:id="@+id/contactus"
        android:layout_width="146dp"
        android:layout_height="41dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="152dp"
        android:background="@color/colorBackground"
        android:text="@string/Contactus"
        android:textColor="@color/colorAccent"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/feedback"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/delivery" />

    <Button
        android:id="@+id/feedback"
        android:layout_width="149dp"
        android:layout_height="41dp"
        android:layout_marginTop="152dp"
        android:layout_marginEnd="8dp"
        android:background="@color/colorBackground"
        android:text="@string/feedback"
        android:textColor="@color/colorAccent"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/contactus"
        app:layout_constraintTop_toBottomOf="@+id/takeaway" />
</androidx.constraintlayout.widget.ConstraintLayout>